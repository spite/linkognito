{
	"name": "Linkognito",
	"description": "Open links in incognito",
	"version": "1.1",
	"background": {
		"scripts": [ "background.js" ]
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["script.js"]
		}
	],
	"permissions": ["tabs", "storage" ],
	"manifest_version": 2
}
